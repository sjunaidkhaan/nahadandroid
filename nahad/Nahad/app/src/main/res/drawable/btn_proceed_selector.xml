<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
            <gradient android:angle="270" android:endColor="#A5D6A7" android:startColor="#A5D6A7" />
            <!-- this is make corners of button rounded -->
            <corners android:bottomLeftRadius="5dip" android:bottomRightRadius="5dip" android:topLeftRadius="5dip" android:topRightRadius="5dip" />
        </shape>
    </item>
    <item android:drawable="@drawable/rounded_rectangle_three_copy"></item>
</selector>